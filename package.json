{
  "name": "react-streaming",
  "version": "0.0.1",
  "main": "dist/index.js",
  "description": "React SSR Streaming. Made Easy.",
  "scripts": {
    "dev": "npm run clean && unbuild --stub && npm run dev:fix-file-extension",
    "dev:fix-file-extension": "rm dist/*.mjs && for f in dist/*.cjs; do mv \"$f\" \"${f%.cjs}.js\"; done",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/"
  },
  "files": [
    "dist/",
    "*.d.ts",
    "*.js"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "react": "^18.0.0",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4"
  },
  "dependencies": {
    "@brillout/json-s": "^0.4.5"
  }
}
