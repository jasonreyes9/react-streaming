{
  "name": "react-streaming",
  "description": "React 18 Streaming. Made fully-fledged & easy.",
  "version": "0.0.1",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "// === Test ===": "",
    "test": "vitest",
    "// === Dev ===": "",
    "dev": "pnpm run tsc:watch:cjs & pnpm run tsc:watch:esm",
    "dev:cjs": "pnpm run tsc:watch:cjs",
    "tsc:watch:cjs": "tsc --incremental --watch --project ./tsconfig.cjs.json",
    "tsc:watch:esm": "tsc --incremental --watch --project ./tsconfig.esm.json",
    "// === Build ===": "",
    "build": "pnpm run clean && tsc --project ./tsconfig.cjs.json && tsc --project ./tsconfig.esm.json",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist/"
  },
  "exports": {
    ".": {
      "node": "./dist/cjs/index.js",
      "browser": "./dist/esm/index.js"
    },
    "./server": {
      "node": "./dist/cjs/server.js"
    },
    "./client": {
      "browser": "./dist/esm/client.js"
    }
  },
  "dependencies": {
    "@brillout/json-s": "^0.4.5",
    "isbot-fast": "^1.2.0",
    "vitest": "^0.9.4"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3"
  },
  "files": [
    "dist/",
    "*.d.ts",
    "*.js"
  ],
  "repository": "github:brillout/react-streaming",
  "license": "MIT"
}
